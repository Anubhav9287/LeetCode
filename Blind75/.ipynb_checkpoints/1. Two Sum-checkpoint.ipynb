    "n = [2,7,11,15]\n",
    "t = 9\n",
    "def twoSum(nums, target):\n",
    "    d = {}\n",
    "    for i,v in enumerate(nums):\n",
    "        diff = target-v\n",
    "        if diff in d:\n",
    "            return (i,d[diff])\n",
    "        d[v] = i"
   ]
